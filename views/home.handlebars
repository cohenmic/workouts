<h1>Workout Tracker</h1>
<section>
	<form id="addForm">
		<fieldset>
			<legend>Add New Workout:</legend>
			<p>
			<label>Enter Name:
				<input type="text" name="name" size="20" maxlength="254">
			</label>
			</p>
			<p>
			<label>Number of Reps:
				<input type="number" name="reps" min="0" max="999999999">
			</label>
			<label>Weight:
				<input type="number" name="weight" min="0" max="9999">
			</label>
			<!--label>Date:
				<input type="date" name="date" size="10" maxlength="10">
			</label-->
			Weight unit:
				<label>pounds
					<input type="radio" name="lbs" value="1" checked/>
				</label>
				<label>kilograms
					<input type="radio" name="lbs" value="0"/>
				</label>
			</p>
			<label>Date:
				<input type="date" name="date" size="10" maxlength="10">
			</label>
			<button type="submit">Submit</button>
		</fieldset>
	</form>
</section>
<section id="workTableSection">
	<h2>Current Record:</h2>
</section>


<script src="main.js"></script>

